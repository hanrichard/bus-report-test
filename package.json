{
  "name": "bus-report-test",
  "version": "1.0.0",
  "type": "module",
  "description": "This is the bus report test",
  "scripts": {
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --write src/",
    "dev": "vite --port 3000 --strictPort",
    "preview": "vite preview --port 3000 --strictPort",
    "serve": "vite preview --port 3000 --strictPort",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "fix": "eslint src --ext .js,.ts,.tsx --max-warnings 0 --fix",
    "test": "export TS_JEST_DISABLE_VER_CHECKER=true && jest --runInBand"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^5.7.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-interactions": "^7.0.27",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/blocks": "^7.0.27",
    "@storybook/builder-vite": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/react-vite": "^7.0.27",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^12.1.10",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.177",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "5.9.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^3.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.13",
    "jest": "^27.0.5",
    "jest-enzyme": "^7.1.2",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "storybook": "^7.0.27",
    "ts-jest": "^27.0.5",
    "typescript": "^5.0.4",
    "vite": "^4.0.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/components/index",
      "!src/setupTests.ts",
      "!src/assets/**/*",
      "!src/*.{ts}",
      "!src/test/**/*",
      "!src/stories/*",
      "!src/index.tsx",
      "!src/**/*.{story,stories}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "ts-jest",
      "^.+\\.svg$": "./svgTransform.ts"
    },
    "testMatch": [
      "**/*test.(ts|tsx)"
    ],
    "setupFilesAfterEnv": [
      "./src/setupTests.ts"
    ]
  }
}
